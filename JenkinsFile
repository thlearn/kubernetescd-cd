node{
  
  try{
  stage('Checkout'){
      checkout scm
     
      }
  stage('compile'){
  
        sh "echo $JAVA_HOME"
        sh "echo $MAVEN_HOME"
        sh "/var/jenkins_home/apache-maven-3.6.1/bin/mvn clean install"
      }
  
     } catch (err) {
      currentBuild.result = 'FAILURE'
    }
}
