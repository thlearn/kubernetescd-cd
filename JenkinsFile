node{
  
  try{
  stage('Checkout'){
      checkout scm
        }
  
 stage("Deploy to Kubernetes"){
          kubernetesDeploy( configs: 'k8s/deployment.yml,k8s/nodeportsvc.yml', kubeConfig: [path: ''], kubeconfigId: 'k8s_deploy')
      
  }

	 }catch (err) {
      currentBuild.result = 'FAILURE'
    }
}

