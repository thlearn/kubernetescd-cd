node{
  
  try{
  stage('Checkout'){
      checkout scm
     
      }
  stage('compile'){
  
        sh "echo $JAVA_HOME"
        sh "echo $MAVEN_HOME"
        sh "mvn clean install"
        sh "cp target/*.war /var/test/jenkins-slave/"
      }
  
     } catch (err) {
      currentBuild.result = 'FAILURE'
    }
}
